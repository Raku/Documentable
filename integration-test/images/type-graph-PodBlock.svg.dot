    "Pod::Block::Declarator" -> "Pod::Block" [color="#000000"];
    "Pod::FormattingCode" -> "Pod::Block" [color="#000000"];
    "Pod::Heading" -> "Pod::Block" [color="#000000"];
    "Pod::Block::Code" -> "Pod::Block" [color="#000000"];
